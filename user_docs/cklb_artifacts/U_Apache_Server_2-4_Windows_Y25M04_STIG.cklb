        {
          "evaluate-stig": {
            "answer_file": "U_Apache_Server_2-4_Windows_Server_STIG_V3R3_Manual-xccdf.xml",
            "last_write": "2025-06-28T19:48:56.364440+00:00Z",
            "afmod": false,
